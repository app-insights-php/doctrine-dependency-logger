{
    "name": "app-insights-php/doctrine-dependency-logger",
    "description": "Microsoft App Insights Doctrine depdendency logger",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/dbal": "^2.3 || ^3.0",
        "app-insights-php/client": "^0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "scripts": {
        "static:analyze": [
            "php-cs-fixer fix --dry-run"
        ],
        "cs:php:fix": [
            "php-cs-fixer fix"
        ]
    }
}
